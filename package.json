{
  "name": "gatsby-source-zesty",
  "version": "0.1.1",
  "description": "Source plugin to get Zesty.io data in Gatsby sites",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/zesty-io/gatsby-source-zesty#readme",
  "scripts": {
    "build": "babel src --out-dir .",
    "prepublishOnly": "yarn run build",
    "lint": "node ./node_modules/eslint/bin/eslint . --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zesty-io/gatsby-source-zesty"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-zesty",
    "plugin",
    "source",
    "zesty",
    "api",
    "cms"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "babel-eslint": "~10.0.1",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0",
    "eslint": "~5.15.1",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-config-prettier": "~4.1.0",
    "eslint-plugin-import": "~2.16.0",
    "eslint-plugin-jsx-a11y": "~6.2.1",
    "eslint-plugin-prettier": "~3.0.1",
    "eslint-plugin-react": "~7.12.4",
    "husky": "~1.3.1",
    "prettier": "~1.16.4",
    "pretty-quick": "~1.10.0",
    "tag-standards": "^0.2.7"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
